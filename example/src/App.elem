<script>
	let name = "a name";
	let anotherName = "worlds";
	$: nameWithSufix = `${name}!!!`;
	$: if (anotherName != "worlds") {
		console.log("Name changed")
		name = `[${name}]`;
	}

	function test() {
		anotherName = `[${anotherName}]`;
	}
</script>

<main test="with {name}" another="test" no-value>
	<h1>Welcome <strong>{anotherName}</strong>!</h1>
	<h1>Another <strong>{nameWithSufix}</strong></h1>
	<button on:click="{test}">Test</button>
</main>
<footer>Test Footer</footer>

